{
  "name": "@simonbiggs/scriptedforms",
  "version": "0.10.0",
  "license": "AGPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonBiggs/scriptedforms.git"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "main": "jlab-build/main.js",
  "types": "jlab-build/main.d.ts",
  "description": "Interactive forms.",
  "author": "Simon Biggs",
  "scripts": {
    "upgrade:jlab": "yarn upgrade --latest --tilde --scope @jupyterlab",
    "upgrade:angular": "yarn upgrade --latest --tilde --scope @angular",
    "upgrade:phosphor": "yarn upgrade --latest --tilde --scope @phosphor",
    "upgrade:types": "yarn upgrade --latest --tilde --scope @types",
    "upgrade:typescript": "yarn upgrade --latest --tilde typescript",
    "ng": "ng",
    "lint": "ng lint",
    "build": "rimraf lib && webpack --config webpack.dev.js",
    "build:prod": "rimraf lib && webpack --config webpack.prod.js",
    "build:jlab": "rimraf ./jlab-build && copyfiles -u 1 './src/**/*.{css,html,scss}' ./jlab-build && tsc && yarn package",
    "watch:jlab": "tsc -w",
    "package": "rimraf simonbiggs-scriptedforms*.tgz && yarn pack",
    "selenium": "./scripts/selenium.sh",
    "test": "yarn --cwd ./e2e e2e",
    "pip:install": "yarn pip:dev",
    "pip:dev": "pip install -e ../",
    "pip:uninstall": "pip uninstall -y scriptedforms",
    "pip:sys": "pip install ../",
    "pip:freeze": "./scripts/pip-freeze.sh",
    "pypi:prep": "yarn build:prod && yarn build:jlab && yarn pip:uninstall && yarn pip:sys && yarn test && yarn pip:dev",
    "pypi:upload": "./scripts/pypi.sh",
    "pypi:post": "yarn pip:uninstall && pip install --pre --no-cache scriptedforms && yarn test && yarn pip:dev",
    "bundle:update": "yarn --cwd ../portable-windows-bundle update",
    "bundle:test": "yarn --cwd ../portable-windows-bundle test",
    "bundle:zip": "yarn --cwd ../portable-windows-bundle zip",
    "watch": "webpack --config webpack.dev.js --watch"
  },
  "files": [
    "jlab-build/**/*.{d.ts,ts,eot,gif,html,css,jpg,js,js.map,json,png,svg,woff2,ttf,scss}"
  ],
  "dependencies": {
    "@angular/animations": "~6.1.4",
    "@angular/cdk": "~6.4.6",
    "@angular/common": "~6.1.4",
    "@angular/compiler": "~6.1.4",
    "@angular/core": "~6.1.4",
    "@angular/forms": "~6.1.4",
    "@angular/http": "~6.1.4",
    "@angular/material": "~6.4.6",
    "@angular/platform-browser": "~6.1.4",
    "@angular/platform-browser-dynamic": "~6.1.4",
    "@angular/router": "~6.1.4",
    "@jupyterlab/application": "~0.18.4",
    "@jupyterlab/apputils": "~0.18.4",
    "@jupyterlab/codemirror": "~0.18.4",
    "@jupyterlab/docmanager": "~0.18.4",
    "@jupyterlab/docregistry": "~0.18.4",
    "@jupyterlab/launcher": "~0.18.4",
    "@jupyterlab/notebook": "~0.18.4",
    "@jupyterlab/outputarea": "~0.18.4",
    "@jupyterlab/rendermime": "~0.18.4",
    "@jupyterlab/services": "~3.1.4",
    "@jupyterlab/theme-light-extension": "~0.18.4",
    "@phosphor/signaling": "~1.2.2",
    "@phosphor/widgets": "~1.6.0",
    "@types/codemirror": "~0.0.59",
    "@types/hammerjs": "^2.0.35",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/markdown-it": "0.0.5",
    "@types/uuid": "^3.4.3",
    "core-js": "^2.5.4",
    "css-loader": "^0.28.11",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.5",
    "json-stable-stringify": "^1.0.1",
    "markdown-it": "^8.4.0",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.8.3",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/cli": "6.1.5",
    "@angular/language-service": "6.1.4",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "codelyzer": "^4.2.1",
    "copyfiles": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.7",
    "karma": "^2.0.0",
    "rimraf": "^2.6.1",
    "svg-inline-loader": "^0.8.0",
    "svg-url-loader": "^2.3.1",
    "tslint": "^5.9.1",
    "typescript": "~2.7.0",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
