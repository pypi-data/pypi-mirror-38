{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "allOf": [
    {"$ref": "response.json"},
    {
      "title": "Results",
      "description": "The results of an analysis",
      "type": "object",
      "properties": {
        "@context": {
          "$ref": "context.json"
        },
        "@type": {
          "default": "results"
        },
        "@id": {
          "description": "ID of the analysis",
          "type": "string"
        },
        "analysis": {
          "default": [],
          "type": "array",
          "items": {
            "anyOf": [
              {
                "$ref": "analysis.json"
              },{
                "type": "string"
              }
            ]
          }
        },
        "entries": {
          "type": "array",
          "default": [],
          "items": {
            "$ref": "entry.json"
          }
        }

      },
      "required": ["@id", "analysis", "entries"]
    }
  ]
}
