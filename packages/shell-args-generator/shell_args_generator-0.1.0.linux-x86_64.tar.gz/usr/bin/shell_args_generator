#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'shell-args-generator==0.1.0','console_scripts','shell_args_generator'
__requires__ = 'shell-args-generator==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('shell-args-generator==0.1.0', 'console_scripts', 'shell_args_generator')()
    )
